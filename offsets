#define OFFSET_UWORLD 0xE0CF488 //GWorld 
#define OFFSET_GAME_INSTANCE 0x1A8 //OwningGameInstance
#define OFFSET_LOCALPLAYERS 0x38 //LocalPlayers
#define OFFSET_PLAYERCONTROLLER 0x30 //UPlayer    PlayerController
#define OFFSET_PAWN 0x328 //APlayerController    AcknowledgedPawn
#define OFFSET_PLAYERSTATE 0x2A8 //APawn    PlayerState
#define OFFSET_ROOTCOMP 0x190 //AActor    RootComponent
#define OFFSET_RELATIVELOCATION 0x128 //USceneComponent    RelativeLocation
#define OFFSET_PERSISTENTLEVEL 0x30 //UWorld    PersistentLevel
 
#define OFFSET_MESH 0x310 //ACharacter    Mesh
#define OFFSET_TEAMINDEX 0x1098 //AFortPlayerStateAthena    TeamIndex 
 
#define OFFSET_CURRENT_WEAPON 0x858 //AFortPawn -> CurrentWeapon
#define OFFSET_WEAPON_DATA 0x3F0 //AFortWeapon -> Weapon Data
#define OFFSET_DISPLAY_NAME 0x90    //constexpr auto DisplayName = 0x90; // FText
#define OFFSET_RARITY 0x70  //Weapon Data -> Rarity

